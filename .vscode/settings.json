{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.customDictionaries": {},
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "package.json"
  ],
  "cSpell.ignoreRegExpList": ["/import\\s+[^;]*;/"],
  "cSpell.language": "fr,en",
  "cSpell.languageSettings": [
    {
      "caseSensitive": false,
      "languageId": "*",
      "locale": "fr"
    }
  ],
  "cSpell.words": ["Tkorp", "toastify", "uniqid", "apk"],

  "prettier.tabWidth": 2,
  "prettier.useTabs": false
}
